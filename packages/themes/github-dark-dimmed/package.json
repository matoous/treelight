{
  "name": "@treelight/theme-github-dark-dimmed",
  "version": "0.0.2",
  "description": "Treelight theme module for github-dark-dimmed.",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matoous/treelight.git"
  },
  "keywords": [
    "treelight",
    "tree-sitter",
    "theme",
    "github-dark-dimmed"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rslib build",
    "build:watch": "rslib build --watch",
    "inspect": "rslib inspect",
    "tsc": "tsc --noEmit",
    "lint": "biome ci --diagnostic-level=error --reporter=github"
  },
  "devDependencies": {
    "@rslib/core": "^0.18.0",
    "@types/node": "^22.19.2",
    "rsbuild-plugin-publint": "^0.3.0",
    "typescript": "^5.7.3"
  }
}
